{
  "manifest_version": 2,
  "name": "Jan K. Pluta",
  "description": "Eksperymentalna strona startowa",
  "version": "2.0.0",
  "icons": { 
    "16": "icon-16x16.png",
    "48": "icon-48x48.png",
    "128": "icon-128x128.png" 
  },
  "author": "Jan K. Pluta",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://api.github.com/*"
  ]
}
